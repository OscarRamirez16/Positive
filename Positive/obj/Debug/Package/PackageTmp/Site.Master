    <script>
        var ctrlKeyDown;
        $(document).ready(function () {
            EstablecerMascaras();
            $(document).on("keydown", keydown);
            $(document).on("keyup", keyup);
            IniciarMenuUsuario();
            setTimeout(function () {
                $('#imgPositive').attr('style', 'top:-5px;position:relative;height:48px');
            }, 2000);
        });
        function keydown(e) { 
			if ((e.which || e.keyCode) == 116 || ((e.which || e.keyCode) == 82 && ctrlKeyDown)) {
				// Pressing F5 or Ctrl+R
				e.preventDefault();
			} else if ((e.which || e.keyCode) == 17) {
				// Pressing  only Ctrl
				ctrlKeyDown = true;
			}
		};
		function keyup(e){
			// Key up Ctrl
			if ((e.which || e.keyCode) == 17)
				ctrlKeyDown = false;
        };

        function IniciarMenuUsuario() {
            var mostrarMenu = localStorage.getItem('mostrarMenu');
            if (mostrarMenu == undefined || mostrarMenu == null) {
                localStorage.setItem('mostrarMenu', 'true');
                mostrarMenu = "true";
            }
            if (mostrarMenu == "false") {
                $("#mySidebar").hide();
                $("#openbtn").html("&#9776;");
                $("#page-wrapper").attr("style", "min-height: 886px; margin:0 0 0 0 !important");

            }
            else {
                $("#mySidebar").show();
                $("#openbtn").html("X");
                $("#page-wrapper").attr("style", "min-height: 886px;");
            }
        }

        function openNav() {
            var mostrarMenu = localStorage.getItem('mostrarMenu');

            if (mostrarMenu == "true") {
                $("#mySidebar").hide();
                $("#openbtn").html("&#9776;");
                $("#page-wrapper").attr("style", "min-height: 886px; margin:0 0 0 0 !important");
                localStorage.setItem('mostrarMenu', 'false');
                
            }
            else {
                $("#mySidebar").show();
                $("#openbtn").html("X");
                $("#page-wrapper").attr("style", "min-height: 886px;");
                localStorage.setItem('mostrarMenu', 'true');
            }
            return false;
        }
    </script>

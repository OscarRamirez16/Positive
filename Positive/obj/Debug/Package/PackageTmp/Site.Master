        /* Position and style the close button (top right corner) */
        .sidebar .closebtn {
          position: absolute;
          top: -15px;
          right: 8px;
          font-size: 36px;
          margin-left: 50px;
        }

        /* The button used to open the sidebar */
        .openbtn {
          font-size: 7px;
          cursor: pointer;
          background-color: #337ab7;
          color: white;
          padding: 10px 10px;
          border: none;
